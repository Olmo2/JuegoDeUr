<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.olmo.JuegoDeUr">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_ur"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_ur"
        android:supportsRtl="true"
        android:theme="@style/Theme.JuegoDeUr">
        <activity
            android:name=".PreferenciasActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".PartidasActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".JuegoActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MenuPrincipal"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>